const clientAdminMg = {
    namespaced: true,
    state: {
        CompanyName: '',
        DepartmentName: '',
        pageIndex: '', 
    },

    mutations: {
        SET_PAGEINDEX:(state, pageIndex) => {
            state.pageIndex = pageIndex;
        },
        SET_CompanyName: (state, CompanyName) => {
            state.CompanyName = CompanyName;
        },
        SET_DepartmentName: (state, DepartmentName) => {
            state.DepartmentName = DepartmentName;
        },
    },
    actions: {
        
    },

    getters: {
        CompanyName: state => state.CompanyName,
        DepartmentName: state => state.DepartmentName,
        pageIndex: state => state.pageIndex,
    }

};



export default clientAdminMg;