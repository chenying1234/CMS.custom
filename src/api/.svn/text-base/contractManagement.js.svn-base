import fetch from '@/utils/fetch';

export function contractList(params){
    return fetch({
        url: '/api/Contract/GetContractList',
        method: 'get',
        params
    })
}
export function contractEdit(data){
    return fetch({
        url: '/api/Contract/EditContract',
        method: 'put',
        data
    })
}
export function contractAdd(data){
    return fetch({
        url: '/api/Contract/AddContract',
        method: 'post',
        data
    })
}
export function contractDelete(params){
    return fetch({
        url: '/api/Contract/DeleteContract',
        method: 'delete',
        params
    })
}
export function contractDetail(params){
    return fetch({
        url: '/api/Contract/GetContractDetail',
        method: 'get',
        params
    })
}

export function GetArchive(params){
    return fetch({
        url: '/api/Contract/GetArchive',
        method: 'get',
        params
    })
}
export function EditArchive(data){
    return fetch({
        url: '/api/Contract/EditArchive',
        method: 'put',
        data
    })
}